<% layout(`layouts/boilerplate`) %>
<h1>All Properties</h1>
<ul>
  <% properties.forEach(property => { %>
    <% const agentIndex = agents.findIndex((agent) => agent.agentCode === property.agentCode); const agent = agents[agentIndex]; %>
    <li><a href="/properties/<%= property._id %>"><%= property.title %></a> listed by <a href="/agents/<%= agent._id %>"><%= agent.name %></a> (<a href="/properties/<%= property._id %>/edit/">Edit</a>)</li>
  <% }) %>
</ul>
